		}
		parm {
			name	"specular_subdivs"
			label	"Specular Subdivs"
			type	integer
			default	{ "8" }
			range	{ 0 32 }
			export	none
			parmtag	{ "cook_dependent" "1" }
			parmtag	{ "vray_plugin" "BRDFSSS2Complex" }
