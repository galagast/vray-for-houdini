		parm {
			name	"front_scatter"
			label	"Front Lighting"
			type	toggle
			default	{ "1" }
			hidewhen	"{ geometry_based_sampling != \"Prepass-Based Illumination Map\" }"
			range	{ 0 1 }
			export	none
			parmtag	{ "cook_dependent" "1" }
			parmtag	{ "vray_plugin" "BRDFSSS2Complex" }
			parmtag	{ "vray_pluginattr" "front_scatter" }
			parmtag	{ "vray_type" "boolean" }
		}
		parm {
			name	"back_scatter"
			label	"Back Lighting"
			type	toggle
			default	{ "1" }
			hidewhen	"{ geometry_based_sampling != \"Prepass-Based Illumination Map\" }"
			range	{ 0 1 }
			export	none
			parmtag	{ "cook_dependent" "1" }
			parmtag	{ "vray_plugin" "BRDFSSS2Complex" }
			parmtag	{ "vray_pluginattr" "back_scatter" }
			parmtag	{ "vray_type" "boolean" }
		}
		parm {
			name	"scatter_gi"
			label	"Scatter Gi"
			type	toggle
			default	{ "0" }
			range	{ 0 1 }
			export	none
			parmtag	{ "cook_dependent" "1" }
			parmtag	{ "vray_plugin" "BRDFSSS2Complex" }
			parmtag	{ "vray_pluginattr" "scatter_gi" }
			parmtag	{ "vray_type" "boolean" }
		}
		parm {
			name	"prepass_blur"
			label	"Prepass Blur"
			type	float
			default	{ "1.2" }
			hidewhen	"{ geometry_based_sampling >= Raytraced }"
			range	{ 0 64 }
			export	none
			parmtag	{ "cook_dependent" "1" }
			parmtag	{ "vray_plugin" "BRDFSSS2Complex" }
			parmtag	{ "vray_pluginattr" "prepass_blur" }
			parmtag	{ "vray_type" "float" }
		}
		parm {
			name	"interpolation_accuracy"
			label	"Interpolation Accuracy"
			type	float
			default	{ "1" }
			help	"Interpolation accuracy for the illumination map; normally 1.0 is fine"
			hidewhen	"{ geometry_based_sampling >= Raytraced }"
			range	{ 0 64 }
			export	none
			parmtag	{ "cook_dependent" "1" }
			parmtag	{ "vray_plugin" "BRDFSSS2Complex" }
			parmtag	{ "vray_pluginattr" "interpolation_accuracy" }
			parmtag	{ "vray_type" "float" }
		}
		parm {
			name	"linear_workflow"
			label	"Linear Workflow"
			type	toggle
			default	{ "0" }
			help	"Set to true to apply the inverse of gamma correction"
			range	{ 0 1 }
			export	none
			parmtag	{ "cook_dependent" "1" }
			parmtag	{ "vray_plugin" "BRDFSSS2Complex" }
			parmtag	{ "vray_pluginattr" "linear_workflow" }
			parmtag	{ "vray_type" "boolean" }
		}
		
		
	
	}
	

	
	
}
